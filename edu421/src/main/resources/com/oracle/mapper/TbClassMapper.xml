  <!-- 查询班级详细信息 -->
  <select id="selectAll" resultType="map">
  SELECT c.*,t.name teacherName,s.STATENAME classType,
cg.NAME class_guideName,jp.NAME job_guideName
 FROM tb_class c LEFT JOIN
tb_teacher t ON c.TEACHERID=t.teacherId LEFT JOIN
tb_state s on c.classTypeId=s.STATEID LEFT JOIN
tb_guide cg on c.CLASS_GUIDEID=cg.GUIDEID
LEFT JOIN tb_guide jp ON c.JOB_GUIDEID=jp.GUIDEID ORDER BY CreatedDate DESC
  </select>
  
  
